{
  "name": "osio-platform-ui",
  "version": "0.0.1",
  "description": "OpenShift.io platform UI",
  "scripts": {
    "lint": "npm-run-all lint:js lint:css",
    "lint:css": "stylelint './packages/**/src/**/*.{css,scss}' --max-warnings 0",
    "lint:js": "eslint './packages/**/src/**/*.{js,ts,tsx}' --max-warnings 0 --format codeframe",
    "format": "npm-run-all format:js format:css format:md",
    "format:css": "npm run lint:css -- --fix",
    "format:js": "npm run lint:js -- --fix",
    "format:md": "prettier '**/*.md' --write",
    "test": "osio-scripts test --env=jsdom",
    "test:watch": "osio-scripts test --env=jsdom --watchAll",
    "test:coverage": "osio-scripts test:coverage --env=jsdom",
    "test:debug": "osio-scripts test:debug --env=jsdom",
    "test:update": "osio-scripts test:update --env=jsdom"
  },
  "keywords": [
    "osio",
    "openshift.io",
    "openshiftio"
  ],
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "graphql": "0.13.2",
    "mobx": "^5.1.0",
    "mobx-react": "^5.2.6",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@osio/scripts": "file:./packages/scripts",
    "@types/jest": "^23.1.3",
    "@types/prop-types": "^15.5.3",
    "@types/react": "16.4.7",
    "@types/react-dom": "^16.0.7",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-osio": "file:./packages/eslint-plugin-osio",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-sort-class-members": "^1.3.1",
    "eslint-plugin-typescript": "^0.12.0",
    "lerna": "^3.4.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "prettier-stylelint-formatter": "^0.5.1-beta.1",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.4.1",
    "stylelint": "^9.5.0",
    "stylelint-config-osio": "file:./packages/stylelint-config-osio",
    "tslib": "^1.9.3",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^20.0.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions",
      "last 2 safari versions",
      "firefox esr"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ]
  }
}
