{
  "name": "ngx-widgets",
  "version": "0.1.0",
  "description": "A collection of Angular components and other useful things to be shared.",
  "main": "bundles/widgets.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "npm run remove-dist && npm run transpile && npm run bundle && npm run minify && npm run copy-files",
    "bundle": "rollup -c",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "cleanup": "rimraf dist/package.json dist/bundles dist/src dist/index.d.ts dist/index.metadata.json dist/index.js dist/index.js.map dist/LICENSE dist/README.md",
    "copy-files": "copyfiles LICENSE README.md package.json dist",
    "minify": "uglifyjs dist/bundles/widgets.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/widgets.umd.min.js",
    "reinstall": "npm run clean && npm install",
    "remove-dist": "rimraf dist",
    "rimraf": "rimraf",
    "transpile": "./node_modules/.bin/ngc"
  },
  "license": "Apache-2.0",
  "contributors": [
