  },
  "bugs": {
    "url": "https://github.com/fabric8-ui/fabric8-ui/issues"
  },
  "engines": {
    "node": ">= 8.3.0",
    "npm": ">= 5.3.0"
  },
  "release": {
    "branch": "master",
    "debug": "true",
    "pkgRoot": "dist"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,ts}"
    ],
    "testMatch": [
      "<rootDir>/src/(app|a-runtime-console|testing)/**/*.(spec|test).{js,ts}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost:8080",
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json",
        "stringifyContentPathRegex": "\\.html?$"
      }
    },
    "transform": {
      "^.+\\.(js)$": "<rootDir>/jest/transforms/babelTransform.js",
      "^.+\\.(ts|html)$": "<rootDir>/jest/transforms/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/jest/transforms/cssTransform.js",
      "^(?!.*\\.(js|css|json|ts|html)$)": "<rootDir>/jest/transforms/fileTransform.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "html"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setupJest.ts",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|ngx|fabric8|patternfly-ng|ng2)"
    ],
    "moduleNameMapper": {
      "^testing/(.*)": "<rootDir>/src/testing/$1"
    }
  }
}
